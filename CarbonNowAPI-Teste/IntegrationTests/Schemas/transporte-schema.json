{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Transporte",
    "type": "object",
    "properties": {
        "valorPesoKg": { "type": "number" },
        "valorDistanciaKm": { "type": "number" },
        "metodoTransporte": { "type": "string" },
        "carbonoKg": { "type": "number" },
        "dataEstimacao": {
            "type": "string",
            "format": "date"
        }
    },
    "required": [ "valorPesoKg", "valorDistanciaKm", "metodoTransporte", "carbonoKg", "dataEstimacao"]
}